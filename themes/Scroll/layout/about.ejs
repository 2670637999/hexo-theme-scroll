<div id="app">
    <div id="content">
        <%- include('nav.ejs') %>
        <div id="title">
            <h1>
                <span style="font-size: 1em;">
                    <%= page.title %><br>
                </span>
                <% if(page.subtitle!=null) { %>
                <p style="margin-top: 10px;">
                    <%= page.subtitle %><br>
                </p>
                <% } %>
            </h1>
        </div>

        <div id="article">
            <%- page.content %>   
            <br>
        </div>
    </div>
</div>