<head>
    <meta charset="utf-8">
    <title><%= config.title %></title>
    <%- css('css/style.css') %>
    <%- js('js/jquery.js') %>
    <%- js('js/main.js') %>

    <% if (page.title) { %>
        <meta name="description" content="<%= page.description %>" />
        <meta name="keywords" content="<%= page.keyword %>" />
    <%} else { %>
        <meta name="description" content="<%= config.description %>" />
        <meta name="keywords" content="<%= config.keyword %>" />
    <% } %>
</head>